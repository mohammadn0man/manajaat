{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "production": {
      "android": { "gradleCommand": ":app:bundleRelease" },
      "distribution": "store"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./fastlane/google-service-account.json"
      }
    }
  }
}
