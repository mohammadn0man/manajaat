{
  "expo": {
    "name": "Munajaat Nomani",
    "slug": "manajaat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/munajaat-nomani.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nomani.munajaat",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/images/munajaat-nomani.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/munajaat-nomani.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nomani.munajaat",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/munajaat-nomani.ico"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/munajaat-nomani.png",
          "color": "#2596be",
          "sounds": []
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Munajaat Nomani needs access to your location to calculate accurate sunrise and sunset times for daily dua reminders."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b1ce03ca-ee38-4bce-a83e-79b7a0d28ce5"
      }
    }
  }
}
